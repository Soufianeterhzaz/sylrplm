<h1><%=t(:volume_edit) %></h1>
<%= form_for(@volume) do |f| %>
<%= f.error_messages %>
<%= render(:partial => "volumes/edit",:locals => { :f => f, :fonct=>"edit" }) %>
<table class='menu_bas'>
    <tr>
        <td>
            <%= f.submit t("update") %>
        </td>
        <td>
            <%= link_to t("explorer"), @volume %>
        </td>
        <td>
            <%= link_to t("list"), volumes_path %>
        </td>
    </tr>
</table>
<% end %>
