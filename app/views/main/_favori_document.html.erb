<div class="favori-title">
	<%= t(:favoris_document)%>
	<%= link_to_remote( h_img(:remove_all).to_s,
	:url=> { :controller=>"documents", :action => :empty_favori }, :method=> :get)%>
</div>
<% unless @favori.get("document").nil? %>
<% if @favori.get("document").count >0 %>
<table>
	<%= render(:partial => "main/favori_item", :collection => @favori.get("document")) %>
</table>

<% end %>
<% end %>

