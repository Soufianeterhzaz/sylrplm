<h1><%=t(:typesobjects_edit) %></h1>
<% form_for(@typesobject) do |f| %>
<%= f.error_messages %>
<%= render(:partial => "typesobjects/edit",:locals => { :f => f, :fonct=>"edit" }) %>

<table class='menu_bas'>
    <tr>
        <td>
            <%= f.submit t("update") %>
