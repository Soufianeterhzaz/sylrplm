<h1><%=t(:sequence_edit) %></h1>
<% form_for(@sequence) do |f| %>
<%= f.error_messages %>
<%= render(:partial => "sequences/edit",:locals => { :f => f, :fonct=>"edit" }) %>
<table class='menu_bas'>
    <tr>
        <td>
            <%= f.submit t("update") %>
        </td>
        <td>
            <%= link_to t("explorer"), @sequence %>
        </td>
        <td>
            <%= link_to t("list"), sequences_path %>
        </td>
    </tr>
</table>
<% end %>
