<h1><%= t(:datafile_add) %></h1>
<% form_for @datafile, :html => { :multipart => true}, :url => { :action=>"add_datafile",:id=>@project.id }  do |f| %>
<%= f.error_messages %>
<%= hidden_field_tag "project_id", @project.id.to_s %>
<%= render(:partial => "datafiles/edit_datafile",:locals => { :f => f , :fonct=>"doc_new", :document=>@project}) %>
<%= render(:partial => "shared/menu_new", :locals => {:object => @datafile, :f => f}) %>
<% end %>